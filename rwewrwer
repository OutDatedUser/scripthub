            if v ~= nil or i ~= nil then
                --getgenv().GettedPet = v
                if getgenv().Url ~= "" and getgenv().PetWebhook == true then
                    local huy = require(game:GetService("ReplicatedStorage").Data.PetData)
                    PWebHook(huy[v].DisplayName, huy[v].Rarity,  huy[v].Multiplier, huy[v].Images.Normal)
                end
            end
        end
    end
end

while getgenv().PetWebhook == true do
    GetPet()
    wait()
