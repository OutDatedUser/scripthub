function GetPet()
    local LmfaoInvoke = game:GetService("ReplicatedStorage").Packages["_Index"]["sleitnick_knit@1.4.7"].knit.Services.EggService.RF.purchaseEgg:InvokeServer("Limited",{})
    if LmfaoInvoke ~= nil then
